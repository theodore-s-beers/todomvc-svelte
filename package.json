{
  "name": "todomvc-svelte",
  "version": "0.1.0",
  "description": "TodoMVC implementation in Svelte, with TypeScript",
  "repository": "github:theodore-s-beers/todomvc-svelte",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "standardize": "find . -name '*.js' ! -path './node_modules/*' ! -path './public/build/*' -exec standard --fix {} ';'",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "dependencies": {
    "sirv-cli": "^1.0.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@tsconfig/svelte": "^1.0.0",
    "@types/uuid": "^8.3.0",
    "rollup": "^2.38.5",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.3",
    "svelte": "^3.32.3",
    "svelte-check": "^1.1.34",
    "svelte-preprocess": "^4.6.8",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}

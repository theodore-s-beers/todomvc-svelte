{
  "name": "todomvc-svelte",
  "version": "0.1.0",
  "private": true,
  "description": "TodoMVC implementation in Svelte, with TypeScript",
  "repository": "github:theodore-s-beers/todomvc-svelte",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "prettify": "prettier --write --plugin-search-dir=. .",
    "standardize": "ts-standard --fix",
    "start": "sirv public --no-clear"
  },
  "dependencies": {
    "sirv-cli": "^2.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.33.0",
    "prettier": "^2.8.3",
    "rollup": "^2.79.1",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "ts-standard": "^12.0.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "ts-standard": {
    "ignore": [
      "*.d.ts"
    ]
  }
}

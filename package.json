{
  "name": "todomvc-svelte",
  "version": "0.1.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "standardize": "find . -name '*.js' ! -path './node_modules/*' ! -path './public/build/*' -exec standard --fix {} ';'",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "dependencies": {
    "sirv-cli": "^1.0.8",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/uuid": "^8.3.0",
    "rollup": "^2.33.3",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.3",
    "svelte": "^3.30.0",
    "svelte-check": "^1.1.17",
    "svelte-preprocess": "^4.6.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}

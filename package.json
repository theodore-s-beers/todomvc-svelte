{
  "name": "todomvc-svelte",
  "version": "0.1.0",
  "private": true,
  "description": "TodoMVC implementation in Svelte, with TypeScript",
  "repository": "github:theodore-s-beers/todomvc-svelte",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "prettify": "prettier --write --plugin-search-dir=. .",
    "standardize": "ts-standard --fix",
    "start": "sirv public --no-clear"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/uuid": "^8.3.1",
    "prettier": "^2.3.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-standard": "^10.0.0",
    "tslib": "^2.0.0",
    "typescript": "4.3.5"
  },
  "ts-standard": {
    "ignore": [
      "*.d.ts"
    ]
  }
}

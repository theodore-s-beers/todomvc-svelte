{
  "name": "todomvc-svelte",
  "version": "0.1.0",
  "private": true,
  "description": "TodoMVC implementation in Svelte, with TypeScript",
  "repository": "github:theodore-s-beers/todomvc-svelte",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "prettify": "prettier --write --plugin-search-dir=. .",
    "standardize": "ts-standard --fix",
    "start": "sirv public --no-clear"
  },
  "dependencies": {
    "sirv-cli": "^2.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "ts-standard": "^11.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "ts-standard": {
    "ignore": [
      "*.d.ts"
    ]
  }
}

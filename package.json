{
  "name": "todomvc-svelte",
  "version": "0.1.0",
  "private": true,
  "description": "TodoMVC implementation in Svelte, with TypeScript",
  "repository": "github:theodore-s-beers/todomvc-svelte",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "rollup -c -w",
    "prettify": "prettier --write --plugin-search-dir=. .",
    "standardize": "ts-standard --fix",
    "start": "sirv public --no-clear"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^2.0.0",
    "@types/uuid": "^8.3.3",
    "prettier": "^2.5.1",
    "rollup": "^2.60.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.2",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "ts-standard": "^11.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.5.3"
  },
  "ts-standard": {
    "ignore": [
      "*.d.ts"
    ]
  }
}
